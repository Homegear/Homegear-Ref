{
	"name": "Homegear::ssdpSearch",
	"category": "General",
	"shortDescription": "Searches for SSDP devices on the network",
	"description": "This method searches for SSDP devices on the network. It sends a <em>M-SEARCH</em> packet and waits for devices to respond. The header field <em>ST</em> of the response must match <em>$stHeader</em>.",
	"signatures": ["bool ssdpSearch(string $stHeader, int $timeout)"],
	"parameters": [{
		"name": "stHeader",
		"type": "string",
		"description": "The value of the header field <em>ST</em> (Search Target) the response must contain. When <em>ssdp:all</em> is used, all responses will be returned.",
		"exampleValue": "urn:schemas-upnp-org:device:ZonePlayer:1"
	}, {
		"name": "timeout",
		"type": "int",
		"description": "The time in milliseconds within the device needs to respond. This value is placed in the header field <em>MX</em> of the <em>M-SEARCH</em> packet.",
		"exampleValue": "5000"
	}],
	"returnValue": "Returns an array containing the parsed responses including the processed description XML if provided.",
	"errors": [{
		"code": "false",
		"description": "Returned on internal errors."
	}],
	"exampleOutput": [{
		"call": "ssdpSearch(\"ssdp:all\", 5000)",
		"output": ""
	}]
}
